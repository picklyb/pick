#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/学习/pick/pick/node_modules/.store/npm-run-all2@6.1.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/学习/pick/pick/node_modules/.store/npm-run-all2@6.1.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/npm-run-all2@6.1.1/node_modules/npm-run-all2/bin/run-s/index.js" "$@"
else
  exec node  "$basedir/../.store/npm-run-all2@6.1.1/node_modules/npm-run-all2/bin/run-s/index.js" "$@"
fi
